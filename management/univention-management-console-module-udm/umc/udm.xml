	<flavor icon="udm-policies" id="policies/policy">
	  <name>Policies</name>
	  <description>Managing policies</description>
	</flavor>
	<flavor icon="udm-navigation" id="navigation">
	  <name>Navigation</name>
	  <description>Browsing the LDAP directory</description>
	</flavor>
	<flavor icon="udm-self" id="users/self">
	  <name>My account</name>
	  <description>Settings for the personal account</description>
	</flavor>
	<categories>
	  <category name="domain"/>
	</categories>
	<command name="udm/license" function="license">
	</command>
	<command name="udm/query" function="query">
	</command>
	<command name="udm/containers" function="containers">
	  <attribute name="superordinate" syntax="String" required="False"/>
	</command>
	<command name="udm/superordinates" function="superordinates">
	</command>
	<command name="udm/types" function="types">
	</command>
	<command name="udm/properties" function="properties">
	  <attribute name="objectType" syntax="String"/>
	</command>
	<command name="udm/options" function="options">
	  <attribute name="objectType" syntax="String"/>
	</command>
	<command name="udm/layout" function="layout">
	  <attribute name="objectType" syntax="String"/>
	</command>
	<command name="udm/values" function="values">
	  <attribute name="objectType" syntax="String"/>
	  <attribute name="objectProperty" syntax="String" required="True"/>
	</command>
	<command name="udm/templates" function="templates">
	  <attribute name="objectType" syntax="String" required="False"/>
	</command>
	<command name="udm/policies" function="policies">
	  <attribute name="objectType" syntax="String" required="True"/>
	</command>
	<command name="udm/put" function="put">
	</command>
	<command name="udm/add" function="add">
	</command>
	<command name="udm/remove" function="remove">
	</command>
	<command name="udm/get" function="get">
	</command>
	<command name="udm/validate" function="validate">
	</command>
