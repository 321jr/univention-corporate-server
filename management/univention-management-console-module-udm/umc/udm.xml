	<flavor icon="udm-users" id="users/user">
	  <name>Users</name>
	  <description>Managing users</description>
	</flavor>
	<flavor icon="udm-groups" id="groups/group">
	  <name>Groups</name>
	  <description>Managing groups</description>
	</flavor>
	<flavor icon="udm-computers" id="computers/computer">
	  <name>Computers</name>
	  <description>Managing computers</description>
	</flavor>
	<flavor icon="udm-networks" id="networks/network">
	  <name>Networks</name>
	  <description>Managing networks</description>
	</flavor>
	<flavor icon="udm-dns" id="dns/dns">
	  <name>DNS</name>
	  <description>Managing DNS settings</description>
	</flavor>
	<flavor icon="udm-dhcp" id="dhcp/dhcp">
	  <name>DHCP</name>
	  <description>Managing DHCP settings</description>
	</flavor>
	<flavor icon="udm-shares" id="shares/share">
