	  <attribute name="syntax" syntax="String" required="True"/>
	</command>
	<command name="udm/nav/container/query" function="nav_container_query">
	</command>
	<command name="udm/nav/object/query" function="nav_object_query">
	</command>
	<command name="udm/object/policies" function="object_policies">
	  <attribute name="objectType" syntax="String" required="True"/>
	  <attribute name="objectDN" syntax="String" required="True"/>
	  <attribute name="policyType" syntax="String" required="True"/>
	</command>
	<command name="udm/object/options" function="object_options">
	  <attribute name="objectType" syntax="String" required="True"/>
	  <attribute name="objectDN" syntax="String" required="False"/>
	</command>
