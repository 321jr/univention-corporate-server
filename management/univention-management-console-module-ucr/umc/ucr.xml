<?xml version="1.0" encoding="UTF-8"?>
<umc version="2.0">
  <module id="ucr" icon="ucr/module" version="1.0">
	<name>Univention Configuration Registry</name>
	<name lang="de_DE">Univention Configuration Registry</name>
	<description>Managing UCR variables</description>
	<description lang="de_DE">Verwaltung von UCR-Variablen</description>
	<flavor icon="ucr/module-expert" id="expert-mode">
	  <option name="extensions" value="True"/>
	  <name>Univention Configuration Registry (Experts)</name>
	  <name lang="de_DE">Univention Configuration Registry (Experten)</name>
	  <description>Managing additional parameters of UCR variables</description>
	  <description lang="de_DE">Verwaltung von zus√§tzlichen Parametern von UCR-Variablen</description>
	</flavor>
	<categories>
	  <category name="system"/>
	</categories>
	<command name="ucr/set" function="set">
	  <attribute name="variables" syntax="UCR-Variables" required="1"/>
	  <return syntax="Boolean"/>
	</command>
	<command name="ucr/unset" function="unset">
	  <attribute name="variable" syntax="UCR-Variable" required="1"/>
	  <return syntax="Boolean"/>
	</command>
	<command name="ucr/get" function="get">
	  <attribute name="variable" syntax="UCR-Variable" required="1"/>
	  <return syntax="Dictionary"/>
	</command>
	<command name="ucr/categories" function="categories">
	  <return syntax="Dictionary"/>
	</command>
	<command name="ucr/search" function="search">
	  <attribute name="filter" syntax="String" required="1"/>
	  <attribute name="key" syntax="UCR-SearchOption" required="1"/>
	  <attribute name="category" syntax="String" required="1"/>
	  <return syntax="UCR-Variables"/>
	</command>
  </module>
</umc>
