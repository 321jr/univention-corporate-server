@%@UCRWARNING=# @%@

@!@
if configRegistry.is_true("self-service/web/enabled", True):
	import re
	ussweb = re.compile('^self-service/(.*)/web/enabled$')
	for plugin in [ussweb.search(k).groups()[0] for k, v in configRegistry.items() if ussweb.search(k) and configRegistry.is_true(k)]:
		print "WSGIScriptAlias /self-service/{0} /usr/share/univention-self-service/web/{0}/wsgi.py".format(plugin)
	print """WSGIDaemonProcess self-service user=self-service group=self-service home=/usr/share/univention-self-service/web/
WSGIProcessGroup self-service"""
@!@
