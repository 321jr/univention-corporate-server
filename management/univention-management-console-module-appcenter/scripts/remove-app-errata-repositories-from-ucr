#!/usr/bin/python2.6



import univention.management.console.modules.appcenter.util as util
import univention.management.console.modules.appcenter.constants as constants
import univention.config_registry as config_registry

if __name__ == "__main__":
	ucr = config_registry.ConfigRegistry()
	ucr.load()
	
	for key, value in ucr.items():
		if key.startswith("repository/online/component/") and key.split("/")[3] != "3.2-0-errata":
			del ucr[key]
	
	ucr.save()





