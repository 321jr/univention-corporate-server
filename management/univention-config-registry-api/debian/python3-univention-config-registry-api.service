[Unit]
Description=univention-config-registry-api
After=network.target

[Install]
WantedBy=multi-user.target

[Service]
User=root
Group=root
Restart=always
ExecStart=/usr/lib/univention-config-registry-api/venv/bin/uvicorn --reload --host 0.0.0.0 --port 3333 univention.config_registry_api.main:app
ExecReload=/bin/kill -s HUP $MAINPID
ExecStop=/bin/kill -s TERM $MAINPID
#WorkingDirectory=/tmp
#Environment=PYTHONPATH='/path/to/mysite'
PrivateTmp=true
