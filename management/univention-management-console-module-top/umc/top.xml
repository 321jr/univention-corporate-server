<?xml version="1.0" encoding="UTF-8"?>
<umc version="2.0">
	<module id="top" icon="top" version="1.0">
		<name>process overview</name>
	<description>process overview</description>
	<categories>
		<category name="system"/>
	</categories>
	<command name="top/query" function="query">
		<attribute name="category" syntax="Top-Category"/>
		<attribute name="filter" syntax="Top-Filter"/>
		<return syntax="Dictionary"/>
	</command>
	<command name="top/kill" function="kill">
		<attribute name="pid" syntax="Integer" required="1"/>
		<attribute name="signal" syntax="Top-Signal" required="1"/>
		<return syntax="Boolean"/>
	</command>
	</module>
</umc>
