<?xml version="1.0" encoding="UTF-8"?>
<umc version="2.0">
	<module id="luga" icon="luga" version="1.0">
	<name>Local user and group administration</name>
	<description>Administrate local users and groups</description>
	<flavor icon="luga-users" id="luga/users">
	  <name>Local users</name>
	  <description>Administrate local users</description>
	</flavor>
	<flavor icon="luga-groups" id="luga/groups">
	  <name>Local groups</name>
	  <description>Administrate local groups</description>
	</flavor>
	<categories>
		<category name="system"/>
	</categories>
	<command name="luga/users/query" function="users_query">
	  <return syntax="List"/>
	</command>
	<command name="luga/users/get_users" function="users_getUsers">
	  <return syntax="List"/>
	</command>
	<command name="luga/users/get" function="users_get">
	  <return syntax="Dictionary"/>
	</command>
	<command name="luga/users/add" function="users_add"/>
	<command name="luga/users/put" function="users_put"/>
	<command name="luga/users/remove" function="users_remove"/>

	<command name="luga/groups/query" function="groups_query">
	  <return syntax="List"/>
	</command>
	<command name="luga/groups/get_groups" function="groups_getGroups">
	  <return syntax="List"/>
	</command>
	<command name="luga/groups/get" function="groups_get">
	  <return syntax="Dictionary"/>
	</command>
	<command name="luga/groups/add" function="groups_add"/>
	<command name="luga/groups/put" function="groups_put"/>
	<command name="luga/groups/remove" function="groups_remove"/>
	</module>
</umc>
