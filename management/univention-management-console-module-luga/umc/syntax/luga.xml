<?xml version="1.0" encoding="UTF-8"?>
<umc version="2.0">
	<definitions>
		<syntax base="Selection" name="luga-groups-search">
			<choice name="groupname"/>
			<choice name="gid"/>
			<choice name="users"/>
			<choice name="administrators"/>
		</syntax>
		<syntax base="Selection" name="luga-users-search">
			<choice name="username"/>
			<choice name="fullname"/>
			<choice name="uid"/>
			<choice name="gid"/>
			<choice name="gecos"/>
			<choice name="shell"/>
			<choice name="homedir"/>
			<choice name="group"/>
		</syntax>
		<syntax base="List" name="luga-users-put">
			<item key="Dictionary" name="luga-users-put-item"/>
		</syntax>
		<syntax base="Dictionary" name="luga-users-put-item">
			<item key="String" value="String"/>
		</syntax>
		<syntax base="String" name="nonempty-value">
			<regex>.+</regex>
		</syntax>
		<syntax base="String" name="Path">
			<regex>.+</regex>
		</syntax>
		<syntax base="String" name="geco">
			<regex>[^,:]*</regex>
		</syntax>
  </definitions>
</umc>
  
