[ldap/acl/user/passwordreset/attributes]
Description[de]=Benutzer-Attribute, die während eines Passwort-Resets durch die spezielle Benutzergruppe modifiziert werden können.
Description[en]=List of user attributes that can be modified by special password reset user group.
Type=str
Categories=service-ldap

[ldap/acl/user/passwordreset/protected/uid]
Description[de]=Liste mit UID, die vor einem Passwort-Reset durch die spezielle Benutzergruppe geschützt sind.
Description[en]=List of UID that protected agains password reset by special user group.
Type=str
Categories=service-ldap

[ldap/acl/user/passwordreset/accesslist/groups/.*]
Description[de]=DN der Benutzergruppe, deren Mitglieder einen Passwort-Reset bei Benutzern durchführen können
Description[en]=DN of user group whose members can perform a password reset for user objects
Type=str
Categories=service-ldap
