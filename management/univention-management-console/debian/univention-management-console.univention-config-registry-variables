Categories=management-umc

[umc/web/timeout/presession]
Description[de]=Laufzeit von Univention Management Console Sitzungen ohne Anmeldung (Anmeldung trotzdem möglich)
Description[en]=Timout of Univention Management Console session without login (login still possible)
Type=str
Categories=management-umc

[umc/web/debug/level]
Description[de]=Debug-Level für das Web-Frontend von UMC
Description[en]=Debug level of the UMC web frontend
Type=int
Categories=management-umc

[umc/web/language]
Description[de]=Sprache für das Web-Frontend von UMC
Description[en]=Language of the UMC web frontend
Type=str
Categories=management-umc

[umc/web/response/timeout]
Description[de]=Defines the amount of time in seconds the web frontend waits for response from the UMC server (default: 30)
Description[en]=Definiert das Zeitintervall in Sekunden für die Wartezeit auf Antworten vom UMC Server (Vorgabe: 30)
Type=str
Categories=management-umc


