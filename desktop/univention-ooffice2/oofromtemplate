#!/bin/sh

source /etc/openoffice/soffice.sh

if [ -x /usr/sbin/univention-baseconfig ]; then
	CUPS=`/usr/sbin/univention-baseconfig get openoffice/cups/support`
	if [ "$CUPS" -a "$CUPS" = "false" ]; then
		export SAL_DISABLE_CUPS=1
	fi
fi

if [ -x /usr/lib/openoffice/program/ooqstart ]; then
	export OOO_EXTRA_ARG=''
	/usr/lib/openoffice/program/ooqstart slot:5500 "$@"
else
	/usr/lib/openoffice/program/soffice $extra_oo_ucs_opts slot:5500 "$@"
fi
