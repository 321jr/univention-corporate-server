@%@UCRWARNING=# @%@
@!@
interfaces = ""
for i in range(10):
	if (configRegistry.get('interfaces/eth%s/type' % i) == 'dhcp') and (configRegistry.get('interfaces/eth%s/ifplugd' % i, 'yes').lower() in ('yes','true','1')):
		interfaces = "%s eth%s" % (interfaces, i)

print 'INTERFACES="%s"' % interfaces.strip()
@!@
HOTPLUG_INTERFACES=""
ARGS="-q -f -u0 -d10 -w -I"
SUSPEND_ACTION="stop"
