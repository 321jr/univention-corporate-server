@%@UCRWARNING=# @%@

@!@
if 'su' in baseConfig['auth/user/services'].split(' '):
	scheme='common'
elif 'su' in baseConfig['auth/admin/services'].split(' '):
	scheme='admin'
else:
	scheme='deny'

print '@include %s-account' % scheme
print 'auth       sufficient pam_rootok.so'
print '@include %s-auth' % scheme
print '@include %s-session' % scheme
print '@include %s-password' % scheme
@!@
