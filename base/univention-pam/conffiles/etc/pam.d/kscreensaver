@%@UCRWARNING=# @%@

@!@
if 'kscreensaver' in baseConfig['auth/user/services'].split(' '):
	scheme='common'
elif 'kscreensaver' in baseConfig['auth/admin/services'].split(' '):
	scheme='admin'
else:
	scheme='deny'

print '@include %s-account' % scheme
print '@include %s-auth-nowrite' % scheme
print '@include %s-session' % scheme
print '@include %s-password' % scheme
@!@
