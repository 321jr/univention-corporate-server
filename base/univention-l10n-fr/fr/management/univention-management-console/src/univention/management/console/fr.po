msgstr "La raison n'a pu être déterminée"

#: src/univention/management/console/protocol/modserver.py:106
#, python-format
msgid "The requested module %r does not exist."
msgstr "Le module %r demandé n'existe pas."

#: src/univention/management/console/protocol/session.py:820
#: src/univention/management/console/protocol/session.py:821
msgid "There are too many opened files on the server."
msgstr "Trop de fichiers ouverts sur le serveur."

#: src/univention/management/console/protocol/session.py:822
msgid "There is not enough free space on the server."
msgstr "Espace disque insuffisant sur le serveur."

#: src/univention/management/console/protocol/session.py:819
msgid "There is not enough memory available on the server."
msgstr "Mémoire disponible insuffisante sur le serveur."

#: src/univention/management/console/base.py:155
msgid "This action requires you to supply your password."
msgstr "Cette action requiert la saisie de votre mot de passe."

#: src/univention/management/console/protocol/definitions.py:109
msgid "Unauthorized"
msgstr "Non autorisé"

#: src/univention/management/console/protocol/message.py:230
#, python-format
msgid "Unknown UMCP command: %s"
msgstr "Commande UMCP inconnue : %s"

#: src/univention/management/console/protocol/definitions.py:125
msgid "Unknown command"
msgstr "Commande inconnue"

#: src/univention/management/console/protocol/message.py:328
