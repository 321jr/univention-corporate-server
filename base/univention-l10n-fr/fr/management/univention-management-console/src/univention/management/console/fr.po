#: src/univention/management/console/modules/sanitizers.py:120
#, python-format
msgid "%d error(s) occurred"
msgstr "%d erreur(s) survenue(s)"

#: src/univention/management/console/modules/decorators.py:627
#, python-format
msgid "%s can only be used as UPLOAD"
msgstr "%s ne peut être utilisé qu'avec « UPLOAD »"

