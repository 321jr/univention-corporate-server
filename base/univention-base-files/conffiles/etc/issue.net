DOMAIN_SERVER = {
	'domaincontroller_master': Univention DC Master",
	'domaincontroller_backup': Univention DC Backup",
	'domaincontroller_slave': Univention DC Slave",
	'memberserver': Univention Memberserver",
}
role = configRegistry.get('server/role')
menutitle = DOMAIN_SERVER.get(role, "Univention Corporate Server")
version = '%(version/version)s-%(version/patchlevel)s' % configRegistry
if all(version.split('-')):
	menutitle += ' %s' % (version,)

print menutitle

