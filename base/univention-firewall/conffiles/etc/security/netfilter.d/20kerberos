#!/bin/sh

@%@BCWARNING=# @%@

@!@
if baseConfig.has_key('security/services/kerberos') and baseConfig['security/services/kerberos'] == 'disabled':
	print '/sbin/iptables -A INPUT  -p tcp --dport 88 -j REJECT'
	print '/sbin/iptables -A INPUT  -p udp --dport 88 -j REJECT'
	print '/sbin/iptables -A INPUT  -p tcp --dport 749 -j REJECT'
@!@
