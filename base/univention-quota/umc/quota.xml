<?xml version="1.0" encoding="UTF-8"?>
<umc version="2.0">
	<module id="quota" icon="quota-module" version="1.0">
	<name>Filesystem quotas</name>
	<description>Set, unset and modify filesystem quota</description>
	<categories>
		<category name="system"/>
	</categories>
	<command name="quota/partitions/query" function="getPartitions">
		<attribute name="partitions" syntax="Dictionary"/>
		<return syntax="Dictionary"/>
	</command>
	<command name="quota/partitions/info" function="getPartitionInfo">
		<attribute name="partitions" syntax="Dictionary"/>
		<return syntax="Dictionary"/>
	</command>
	<command name="quota/partitions/activate" function="activatePartitions">
		<attribute name="partitions" syntax="Dictionary"/>
		<return syntax="Dictionary"/>
	</command>
	<command name="quota/partitions/deactivate" function="deactivatePartitions">
		<attribute name="partitions" syntax="Dictionary"/>
		<return syntax="Dictionary"/>
	</command>
	<command name="quota/users/query" function="getUsers">
		<attribute name="partitions" syntax="Dictionary"/>
		<return syntax="Dictionary"/>
	</command>
	<command name="quota/users/set" function="setUser">
		<attribute name="user" syntax="Quota-User"/>
		<attribute name="partition" syntax="Quota-Partition"/>
		<attribute name="block_soft" syntax="Quota-Block-Soft"/>
		<attribute name="block_hard" syntax="Quota-Block-Hard"/>
		<attribute name="file_soft" syntax="Quota-File-Soft"/>
		<attribute name="file_hard" syntax="Quota-File-Hard"/>
		<return syntax="Dictionary"/>
	</command>
	<command name="quota/users/remove" function="removeUsers">
		<attribute name="user" syntax="Quota-User"/>
		<attribute name="partition" syntax="Quota-Partition"/>
		<return syntax="Dictionary"/>
	</command>
	</module>
</umc>
