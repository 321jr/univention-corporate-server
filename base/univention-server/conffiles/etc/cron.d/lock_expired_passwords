if configRegistry.get('server/role') == 'domaincontroller_master':
	interval = configRegistry.get('directory/manager/user/lock_expired_passwords/cron/entry', '23 3 * * *')
	disabled = '# '
	if configRegistry.is_true('directory/manager/user/lock_expired_passwords/cron/enabled', False):
		disabled = ''
	print '%s%s   root   /usr/share/univention-directory-manager-tools/lock_expired_passwords --quiet' % (disabled, interval)
