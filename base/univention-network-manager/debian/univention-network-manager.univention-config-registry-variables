[networkmanager/group]
Description[de]=Gruppe für den Zugriff auf den Network Manager
Description[en]=Group with access to the network manager
Type=str
Categories=system-base

[networkmanager/all_interfaces]
Description[de]=Definiert, ob alle Netzwerk-Interfaces durch den NetworkManager verwaltet werden sollen
Description[en]=defines if all network interfaces should be managed by the NetworkManager
Type=bool
Categories=system-base

[networkmanager/action/mount]
Description[de]=Ist diese Variable gesetzt, dann wird versucht die Heimatverzeichnisfreigabe per NFS einzubinden
Description[en]=If this variable is set the home directory share will be mounted via NFS if possible
Type=bool
Categories=system-base

[networkmanager/dhcp/options/fallback]
Description[de]=Benutzt alternative Werte, die nicht vom DHCP Server übermittelt werden
Description[en]=Use fallback values not provided by DHCP server
Type=bool
Categories=deprecated
