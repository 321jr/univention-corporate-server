#!/bin/sh

# Automatically added by univention-install-config-registry
if [ "$1" = "abort-remove" ]; then
  [ -e /etc/univention/templates/removed/univention-system-check-downloader.info ] && mv /etc/univention/templates/removed/univention-system-check-downloader.info /etc/univention/templates/info/univention-system-check-downloader.info || true
fi
[ -x /usr/sbin/univention-config-registry ] && univention-config-registry register univention-system-check-downloader || true
# End automatically added section


ucr set univention-system-check-downloader/enabled=true
ucr set univention-system-check-downloader/cron='23 59 * * *'

exit 0
