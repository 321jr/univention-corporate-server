	oplocks = @%@samba/oplocks@%@
	kernel oplocks = @%@samba/kernel_oplocks@%@
	large readwrite = @%@samba/large_readwrite@%@
	deadtime = @%@samba/deadtime@%@
	read raw = @%@samba/read_raw@%@
	write raw = @%@samba/write_raw@%@
	max xmit = @%@samba/max_xmit@%@
	getwd cache = @%@samba/getwd_cache@%@

	store dos attributes = @%@samba/store_dos_attributes@%@
	

@!@
if baseConfig['server/role'] != 'memberserver':
	if ( not baseConfig['samba/homedirpath'] or baseConfig['samba/homedirpath']=='local') and baseConfig['samba/homedirserver'] and baseConfig['samba/homedirserver'] == 'local':
		print '\tlogon home ='
	elif baseConfig['samba/homedirserver'] and baseConfig['samba/homedirpath']:
		print '\tlogon home = \\\\%s\%s' % (baseConfig['samba/homedirserver'],baseConfig['samba/homedirpath'])

	if baseConfig['samba/homedirletter'] and len(baseConfig['samba/homedirletter']) == 1:
		print '\tlogon drive = %s:' % baseConfig['samba/homedirletter']
	elif baseConfig['samba/homedirletter'] and baseConfig['samba/homedirletter'] == 'local':
		print '\tlogon drive = '
		
	if ( not baseConfig['samba/profilepath'] or baseConfig['samba/profilepath'] == 'local' ) and baseConfig['samba/profileserver'] and baseConfig['samba/profileserver'] == 'local':
		print '\tlogon path ='
	elif baseConfig['samba/profileserver'] and baseConfig['samba/profilepath']:
		print '\tlogon path = \\\\%s\%s' % (baseConfig['samba/profileserver'],baseConfig['samba/profilepath'])

	if baseConfig.has_key('samba/logonscript'):
		print '\tlogon script = %s' % baseConfig['samba/logonscript']


@!@

	preserve case = @%@samba/preserve_case@%@
	short preserve case = @%@samba/short_preserve_case@%@

	time server = @%@samba/time_server@%@

@!@
if baseConfig.has_key('samba/enable-msdfs') and baseConfig['samba/enable-msdfs'] == 'yes':
	print '\thost msdfs = yes'
else:
	print '\thost msdfs = no'
	print '\tmsdfs root = no'
@!@
