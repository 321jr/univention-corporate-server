
@!@
if baseConfig.has_key('cups/server') and baseConfig['cups/server'] in ['127.0.0.1','localhost','%s' % baseConfig['hostname'],'%s.%s' % (baseConfig['hostname'],baseConfig['domainname'])]:
    print '\t; printing'
    print '\tload printers = yes'
    print '\tprinting = cups'
    print '\tprintcap name = cups '
if baseConfig.get('samba/cups/encrypt'):
	print '\tcups encrypt = %s' % baseConfig.get('samba/cups/encrypt')
