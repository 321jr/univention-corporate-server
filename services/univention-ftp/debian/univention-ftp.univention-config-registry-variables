[ftp/chroot.*]
Description[de]=Benutzer, die bei FTP-Zugriff eine chroot-Umgebung erhalten
Description[en]=Users that will be put into a chroot environment for ftp access
Type=str
Categories=Network

[ftp/deny.*]
Description[de]=Benutzer, die keinen FTP-Zugriff erhalten
Description[en]=Users that are denied ftp access
Type=str
Categories=Network

[ftp/denyall]
Description[de]=Allen Benutzern den FTP-Zugriff verweigern
Description[en]=Deny ftp access to all users
Type=str
Categories=Network
