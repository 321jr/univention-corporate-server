if ! test -e "/usr/share/univention-join/.joined"; then
	echo "The server has not joined yet"
	exit 1
fi

touch /usr/lib/univention-install/.index.txt

test -n "`grep "univention-printserver successful" /usr/lib/univention-install/.index.txt`" && exit 1

eval `univention-baseconfig shell`

univention-admin container/cn create $@ --ignore_exists --position "cn=univention,$ldap_base" --set name=cups

univention-admin settings/printermodel create $@ --ignore_exists --position "cn=cups,cn=univention,$ldap_base" --set name=misc  \
	--append printmodel="\"None\" \"None\"" \
	--append printmodel="\"smb\" \"smb\""

