<!-- $Horde: framework/Cache/package.xml,v 1.6 2003/10/29 19:35:01 chuck Exp $ -->
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.1">
<package version="1.0">
  <name>Horde_Cache</name>
  <summary>Caching API</summary>
  <description>
    This package provides a simple, functional Caching API, with the option to store the cached data either on the filesystem, or using the Zend Performance Suite's content cache.
  </description>
  <license>LGPL</license>
  <maintainers>
    <maintainer>
      <user>chuck</user>
      <role>lead</role>
      <name>Chuck Hagenbuch</name>
      <email>chuck@horde.org</email>
    </maintainer>
  </maintainers>
  <release>
    <version>0.0.1</version>
    <state>alpha</state>
    <date>2003-02-11</date>
    <notes>Initial packaging.</notes>
    <filelist>
      <file role="php" baseinstalldir="/Horde" name="Cache.php" />
      <dir baseinstalldir="/Horde" name="Cache" role="php">
        <file name="file.php" />
        <file name="zps.php" />
      </dir>
    </filelist>
    <deps>
      <dep type="php" rel="ge" version="4.2.0+" />
      <dep type="pkg" rel="has">Horde_Util</dep>
    </deps>
  </release>
</package>
