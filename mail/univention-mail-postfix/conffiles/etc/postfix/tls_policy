for key, value in [x for x in configRegistry.items() if x[0].startswith('mail/postfix/tls/client/policy')]:
	target = key.split('/')[-1]
	if target == 'nomx':
		target = key.split('/')[-2].split(':')
		target = '[{}]{}'.format(target[0], ':{}'.format(target[1]) if len(target) > 1 else '')
	print '{} {}'.format(target, value)
@!@