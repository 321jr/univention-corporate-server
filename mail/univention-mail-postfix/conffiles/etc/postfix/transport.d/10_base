import re
for key in configRegistry.keys():
	if key.startswith('mail/maps/transport/'):
		val = configRegistry[key].strip()
		if ' ' in val:
			domain, transp = val.split(' ', 1)
			print '%-30s %s' % (domain, transp)
		else:
			print val

