<package version="1.0">
  <name>Menu</name>
  <summary>Horde Menu package</summary>
  <description>
    Package for creating and manipulating Horde menus and menuitems.
  </description>
  <license>LGPL</license>

  <maintainers>
    <maintainer>
      <user>chuck</user>
      <role>lead</role>
      <name>Chuck Hagenbuch</name>
      <email>chuck@horde.org</email>
    </maintainer>
