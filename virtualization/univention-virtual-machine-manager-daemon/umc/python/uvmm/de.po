msgstr "Unbekanntes Profil wurde angegeben"

#: umc/python/uvmm/__init__.py:150
msgid "Unknown query type"
msgstr "Unbekannter Anfragetyp"

#: umc/python/uvmm/domains.py:505
msgid "vcpus must be a number"
msgstr "vcpus muß numerisch sein"

