#: umc/python/uvmm/domains.py:416
msgid "Could not determine architecture for domain"
msgstr "Die Architektur der Instanz konnte nicht ermittelt werden"

#: umc/python/uvmm/domains.py:530
msgid "Could not determine the keyboard layout for the VNC access"
msgstr ""
"Das Tastaturlayout für den VNC Direktzugriff konnte nicht ermittelt werden"

#: umc/python/uvmm/domains.py:514
msgid "Could not determine the list of boot devices for domain"
msgstr "Die Liste der Boot-Devices der Instanz konnte nicht ermittelt werden"

#: umc/python/uvmm/domains.py:486
msgid "Could not determine virtualization technology for domain"
msgstr ""
"Die Virtualisierungstechnologie der Instanz konnte nicht ermittelt werden"

#: umc/python/uvmm/storages.py:335
#, python-format
msgid "Could not retrieve details for domain %s"
msgstr "Die Details zur virtuellen Instanz %s konnten nicht abgerufen werden"

#: umc/python/uvmm/storages.py:317
msgid "Failed to check if the drive is used by any other virtual instance"
msgstr ""
"Die Prüfung ob das Laufwerk von einer anderen virtuellen Instanz verwendet "
"wird ist fehlgeschlagen."

#: umc/python/uvmm/domains.py:463
#, python-format
msgid "Failed to retrieve details for the server %(nodeURI)s"
msgstr ""
"Das Abrufen der Details zum physikalischen Server %(nodeURI)s ist "
"fehlgeschlagen"

#: umc/python/uvmm/profiles.py:77
msgid "Full virtualization (KVM)"
msgstr "Voll-Virtualisierung (KVM)"

#: umc/python/uvmm/profiles.py:78
msgid "Full virtualization (XEN)"
msgstr "Voll-Virtualisierung (XEN)"

#: umc/python/uvmm/domains.py:626
#, python-format
msgid "Invalid domain state: %s"
msgstr "Ungültiger Domain Status: %s"

#: umc/python/uvmm/profiles.py:79
msgid "Paravirtualization (XEN)"
msgstr "Paravirtualisierung (XEN)"

#: umc/python/uvmm/nodes.py:102 umc/python/uvmm/__init__.py:145
msgid "Physical servers"
msgstr "Physikalische Server"

#: umc/python/uvmm/uvmmd.py:68
msgid "The given UVMM command is not known"
msgstr "Der gegebene UVMM Befehl ist unbekannt"

#: umc/python/uvmm/storages.py:255
msgid "The given pool could not be found or is no file pool"
msgstr ""
"Der angegebene Speicherbereich konnte nicht gefunden werden oder ist kein "
"Verzeichnis"

#: umc/python/uvmm/domains.py:467
#, python-format
msgid "Unknown physical server %(nodeURI)s"
msgstr "Unbekannter physikalischer Server %(nodeURI)s"

#: umc/python/uvmm/profiles.py:176
msgid "Unknown profile"
msgstr "Unbekanntes Profil"

#: umc/python/uvmm/domains.py:437
