@%@BCWARNING=# @%@

# /var/log/univention/uvmmd.log
/var/log/univention/virtual-machine-manager-daemon.log {
	rotate @%@log/rotate/weeks@%@
	weekly
	compress
	delaycompress
	missingok
	notifempty
	create 640 root adm
	postrotate
		if [ -e /var/run/uvmmd.pid ]
		then
			kill -HUP "`cat /var/run/uvmmd.pid`"
		fi
	endscript
}
